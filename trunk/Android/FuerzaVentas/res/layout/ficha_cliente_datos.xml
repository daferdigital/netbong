        <EditText android:textColor="#ff000000" android:id="@id/et_contacto_telef" android:layout_width="fill_parent" android:layout_height="wrap_content" android:lines="2" android:inputType="phone" />
        <TextView android:textSize="24.0dip" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Contacto - Fax:" />
        <EditText android:textColor="#ff000000" android:id="@id/et_contacto_fax" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="" android:lines="2" />
        <TextView android:textSize="24.0dip" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Contacto - Correo:" />
        <EditText android:textColor="#ff000000" android:id="@id/et_contacto_correo" android:layout_width="fill_parent" android:layout_height="wrap_content" android:lines="2" android:inputType="textEmailAddress" />
