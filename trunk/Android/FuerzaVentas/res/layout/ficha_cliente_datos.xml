    </LinearLayout>
    <LinearLayout android:orientation="vertical" android:padding="15.0dip" android:layout_width="fill_parent" android:layout_height="wrap_content"
      xmlns:android="http://schemas.android.com/apk/res/android">
        <TextView android:textSize="24.0dip" android:id="@id/textView1" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Direccion Fiscal:" />
        <EditText android:textColor="#ff000000" android:id="@id/et_dir_fiscal" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="" android:lines="3" />
    </LinearLayout>
    <LinearLayout android:orientation="vertical" android:padding="15.0dip" android:layout_width="fill_parent" android:layout_height="wrap_content"
